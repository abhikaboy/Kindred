{
    "expo": {
        "name": "Kindred",
        "slug": "Kindred",
        "version": "0.0.1",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "scheme": "kindred",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "platforms": ["ios", "android"],
        "ios": {
            "supportsTablet": true,
            "infoPlist": {
                "NSLocationWhenInUseUsageDescription": "We use your location to look up nearby restaurants and addresses.",
                "ITSAppUsesNonExemptEncryption": false,
                "NSAppTransportSecurity": {
                    "NSAllowsArbitraryLoads": true,
                    "NSExceptionDomains": {
                        "157.230.81.185": {
                            "NSTemporaryExceptionAllowsInsecureHTTPLoads": true
                        }
                    }
                }
            },
            "entitlements": {
                "aps-environment": "development"
            },
            "usesAppleSignIn": true,
            "bundleIdentifier": "com.kindred.kindredtsl",
            "buildNumber": "6"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-apple-authentication",
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/splash-icon.png",
                    "imageWidth": 100,
                    "dark": {
                        "image": "./assets/splash-icon.png",
                        "backgroundColor": "#13121F"
                    },
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff"
                }
            ],
            "expo-secure-store"
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "f64f0563-7cb2-45c4-b928-800e53bc5e10"
            }
        }
    }
}
